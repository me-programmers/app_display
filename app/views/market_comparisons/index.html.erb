<h1>Edit Harga Komoditi</h1>

<table id="customers">
  <tr>
    <th>No</th>
    <th>Komoditi</th>
    <th>Grosir1 (Pasar Terong)</th>
    <th>Grosir2 (Pasar Ujung)</th>
    <th>Petani</th>
    <th>Eceran</th>
    <th>Jenis</th>
    <th></th>
  </tr>

<% @market_comparisons.each do |market_comparison| %>
  <tr class="alt">
    <td><%= market_comparison.id %></td>
    <td><%= market_comparison.komoditi %></td>
    <td><%= number_with_precision(market_comparison.grosir1, :delimiter => ".", :precision => 0) %></td>
    <td><%= number_with_precision(market_comparison.grosir2, :delimiter => ".", :precision => 0) %></td>
    <td><%= number_with_precision(market_comparison.petani, :delimiter => ".", :precision => 0) %></td>
    <td><%= number_with_precision(market_comparison.eceran, :delimiter => ".", :precision => 0) %></td>
    <td><%= market_comparison.jenis %></td>
    <!--<td><%= link_to 'Show', market_comparison %></td>-->
    <td><%= link_to 'Edit', edit_market_comparison_path(market_comparison), :class => "btn"  %></td>
    <!--<td><%= link_to 'Destroy', market_comparison, method: :delete, data: { confirm: 'Are you sure?' } %></td>-->
  </tr>
<% end %>
</table>

<br />

